{
    "name": "In the Depths",
    "type": "dark",
    "colors": {
        "editor.background": "#222725",
        "editor.foreground": "#747c80",
        "editor.selectionBackground": "default",
        "editor.lineHighlightBackground": "default",
        "editorCursor.foreground": "#B8B8B8",
        "editorWhitespace.foreground": "default",
        "editorIndentGuide.background1": "#3A4145",
        "editorIndentGuide.activeBackground1": "#5A6267",
        "editor.selectionHighlightBackground": "default",
        "statusBar.background": "#222725",
        "statusBar.foreground": "#B8B8B8",
        "titleBar.activeBackground": "#222725",
        "titleBar.activeForeground": "#B8B8B8",
        "activityBar.background": "#222725",
        "activityBar.foreground": "#B8B8B8",
        "sideBar.background": "#222725",
        "sideBar.foreground": "#B8B8B8"
    },
    "tokenColors": [
        {
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator",
                "keyword.other",
                "keyword.new",
                "keyword.async"
            ],
            "settings": {
                "foreground": "#82689b",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "variable",
                "variable.other",
                "variable.language",
                "variable.other.local",
                "variable.other.parameter"
            ],
            "settings": {
                "foreground": "#A0A0A0"
            }
        },
        {
            "scope": [
                "constant",
                "constant.language",
                "constant.numeric",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#67a088"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted",
                "string.quoted.double",
                "string.quoted.single"
            ],
            "settings": {
                "foreground": "#819475"
            }
        },
        {
            "scope": ["comment", "comment.block", "comment.line"],
            "settings": {
                "foreground": "#707070",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "meta.function",
                "variable.function"
            ],
            "settings": {
                "foreground": "#7d90c5"
            }
        },
        {
            "scope": ["entity.name.class", "entity.name.type"],
            "settings": {
                "foreground": "#a37c9b",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "punctuation",
                "punctuation.separator",
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#6E6E6E"
            }
        },
        {
            "scope": ["variable.other.property", "variable.other.object"],
            "settings": {
                "foreground": "#73949b"
            }
        },
        {
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#FF6F6F"
            }
        },
        {
            "scope": [
                "meta.tag",
                "meta.link",
                "meta.selector",
                "meta.property-name"
            ],
            "settings": {
                "foreground": "#A0A0A0"
            }
        },
        {
            "scope": ["variable.other.color", "variable.other.constant"],
            "settings": {
                "foreground": "#A0A0A0"
            }
        },
        {
            "scope": [
                "keyword.operator.logical",
                "keyword.operator.comparison"
            ],
            "settings": {
                "foreground": "#A0A0A0"
            }
        },
        {
            "scope": [
                "variable.other.import",
                "variable.other.js",
                "variable.other.scss",
                "variable.other.ts"
            ],
            "settings": {
                "foreground": "#FFAB6F"
            }
        },
        {
            "scope": ["variable.language.boolean", "variable.other.state"],
            "settings": {
                "foreground": "#F5A700"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "entity.name.tag.js",
                "entity.name.tag.jsx"
            ],
            "settings": {
                "foreground": "#a56882",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "entity.name.tag.section",
                "entity.name.tag.div",
                "entity.name.tag.header",
                "entity.name.tag.footer",
                "entity.name.tag.article",
                "entity.name.tag.aside"
            ],
            "settings": {
                "foreground": "#7BB9C5"
            }
        },
        {
            "scope": [
                "meta.jsx.children",
                "meta.jsx.attribute",
                "meta.jsx.self-closing",
                "meta.jsx.end-tag"
            ],
            "settings": {
                "foreground": "#A0A0A0"
            }
        },
        {
            "scope": ["entity.name.component", "entity.name.jsx"],
            "settings": {
                "foreground": "#a37c9b"
            }
        },
        {
            "scope": ["variable.other.jsx", "variable.other.js"],
            "settings": {
                "foreground": "#F5A700"
            }
        },
        {
            "scope": ["entity.other.attribute-name.css"],
            "settings": {
                "foreground": "#66c2c2",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["support.type.property-name.css"],
            "settings": {
                "foreground": "#8b96c5"
            }
        },
        {
            "scope": ["entity.name.tag.css"],
            "settings": {
                "foreground": "#a37c9b"
            }
        },
        {
            "scope": [
                "support.constant.property-value.css",
                "support.constant.property-value.scss"
            ],
            "settings": {
                "foreground": "#76c7b7"
            }
        },
        {
            "scope": ["support.variable.color.css"],
            "settings": {
                "foreground": "#ff7373"
            }
        },
        {
            "scope": ["keyword.control.import.ts", "keyword.control.export.ts"],
            "settings": {
                "foreground": "#82689b",
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["support.type.primitive.ts"],
            "settings": {
                "foreground": "#747c80"
            }
        },
        {
            "scope": ["entity.name.type.ts"],
            "settings": {
                "foreground": "#a37c9b",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name",
                "entity.other.attribute-name.id",
                "entity.other.attribute-name.class"
            ],
            "settings": {
                "foreground": "#888888",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.selector",
                "meta.selector.class",
                "meta.selector.id"
            ],
            "settings": {
                "foreground": "#FF6347",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["entity.other.attribute-name.class.tailwind"],
            "settings": {
                "foreground": "#9F7AEA"
            }
        },
        {
            "scope": ["variable.language.next"],
            "settings": {
                "foreground": "#ff7e5f"
            }
        }
    ]
}
